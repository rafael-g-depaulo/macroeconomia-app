{
  "name": "@starter-project/server",
  "version": "1.3.0",
  "main": "dist/server.cjs.js",
  "module": "dist/server.esm.js",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/dotenv -- if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "dev": "yarn start:dev",
    "prod": "yarn start:prod",
    "start:dev": "./node_modules/.bin/dotenv -- nodemon --exec babel-node --root-mode upward index.js",
    "start:prod": "./node_modules/.bin/dotenv -- yarn serve",
    "serve": "node ./build",
    "build": "yarn build:server && yarn build:db",
    "build:server": "./node_modules/.bin/babel . -d ./build -s",
    "build:db": "yarn db:migrate",
    "db:create": "./node_modules/.bin/sequelize db:create",
    "db:drop": "./node_modules/.bin/sequelize db:drop",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:unseed": "./node_modules/.bin/sequelize db:seed:undo:all",
    "db:reseed": "yarn db:unseed && yarn db:seed",
    "db:setup": "./node_modules/.bin/sequelize db:create && yarn db:migrate && yarn db:seed",
    "db:reset": "yarn db:drop && yarn db:setup",
    "test": "jest",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@starter-project/logger": "^1.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.4",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@preconstruct/cli": "^1.1.19",
    "nodemon": "^2.0.2"
  },
  "engines": {
    "node": "12.x",
    "yarn": "1.x"
  },
  "preconstruct": {
    "source": "index"
  }
}
